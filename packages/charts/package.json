{
  "name": "@wonderflow/charts",
  "version": "12.6.0",
  "description": "Charts Library from Wonderflow's Wanda design system",
  "author": "Wonderflow",
  "license": "Apache-2.0",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "core",
    "core.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=8.x"
  },
  "scripts": {
    "prebuild": "run-s clean build:csstypes",
    "build": "run-s build:ts build:css",
    "postbuild": "rimraf dist/**/_mixins.css",
    "build:ts": "tsc && tsc-alias",
    "build:css": "cpy '**/*.css' '../dist' --parents --cwd=src && postcss dist/*.css \"dist/**/!(utils)/!(_mixins).css\" --replace",
    "lint": "run-s lint:styles",
    "lint:styles": "stylelint ./src/**/*.css",
    "clean": "rimraf dist core core.css",
    "watch:csstypes": "tcm -w -p '**/*.module.css' src",
    "build:csstypes": "tcm -p '**/*.module.css' src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/wanda.git"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "keywords": [
    "charts",
    "wonderflow",
    "library",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/wanda/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/wanda",
  "peerDependencies": {
    "react": ">=17.0",
    "react-dom": ">=17.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@types/d3": "7.4.1",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-table": "7.7.12",
    "@types/react-test-renderer": "18.0.0",
    "@types/uuid": "9.0.2",
    "babel-loader": "8.2.5",
    "cpy-cli": "3.1.1",
    "postcss": "8.4.17",
    "postcss-cli": "10.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tsconfig": "10.0.0",
    "typed-css-modules": "0.7.2",
    "typescript": "4.8.4",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "dependencies": {
    "@visx/annotation": "3.3.0",
    "@visx/axis": "3.3.0",
    "@visx/curve": "3.3.0",
    "@visx/event": "3.3.0",
    "@visx/gradient": "3.3.0",
    "@visx/grid": "3.3.0",
    "@visx/group": "3.3.0",
    "@visx/mock-data": "3.3.0",
    "@visx/responsive": "3.3.0",
    "@visx/scale": "3.3.0",
    "@visx/shape": "3.3.0",
    "@visx/text": "3.3.0",
    "@visx/tooltip": "3.3.0",
    "@visx/vendor": "3.3.0",
    "ahooks": "3.7.5",
    "clsx": "1.2.1",
    "constate": "3.3.2",
    "d3": "7.8.5",
    "framer-motion": "6.5.1",
    "lodash": "4.17.21",
    "react-focus-on": "3.6.0",
    "react-merge-refs": "2.0.1",
    "react-paginate": "8.1.3",
    "react-popper-tooltip": "4.4.2",
    "react-roving-tabindex": "3.2.0",
    "react-uid": "2.3.2",
    "slugify": "1.6.5",
    "type-fest": "3.0.0",
    "uuid": "9.0.0"
  }
}
