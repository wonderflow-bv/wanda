{
  "name": "@wonderflow/react-components",
  "version": "3.1.3",
  "description": "UI components from Wonderflow's Wanda design system",
  "author": "Wonderflow",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "core",
    "core.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=8.x"
  },
  "scripts": {
    "start": "start-storybook --quiet -p 6006",
    "prebuild": "rimraf dist core core.css",
    "build": "run-s build:ts build:css",
    "postbuild": "rimraf dist/**/_mixins.css",
    "build:ts": "tsc",
    "build:css": "cpy '**/*.css' '../dist' --parents --cwd=src && postcss dist/*.css \"dist/**/!(utils)/!(_mixins).css\" --replace",
    "postbuild:css": "mv dist/core.css ./core.css && mv dist/core ./core",
    "lint": "run-s lint:styles",
    "lint:styles": "stylelint ./src/**/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderflow-bv/wanda.git"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "keywords": [
    "design-system",
    "wonderflow",
    "components",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/wonderflow-bv/wanda/issues"
  },
  "homepage": "https://github.com/wonderflow-bv/wanda",
  "peerDependencies": {
    "react": "^17.0",
    "react-dom": "^17.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.16.7",
    "@storybook/addon-actions": "6.5.0-alpha.4",
    "@storybook/addon-essentials": "6.5.0-alpha.4",
    "@storybook/addon-links": "6.5.0-alpha.4",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.5.0-alpha.4",
    "@storybook/builder-webpack5": "6.5.0-alpha.4",
    "@storybook/manager-webpack5": "6.5.0-alpha.4",
    "@storybook/react": "6.5.0-alpha.4",
    "@storybook/theming": "6.5.0-alpha.4",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@wonderflow/stylelint-config": "1.1.4",
    "@wonderflow/tokens": "*",
    "cpy-cli": "3.1.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.0.2",
    "postcss-mixins": "9.0.1",
    "postcss-preset-env": "7.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "storybook-addon-themes": "6.1.0",
    "storybook-css-modules-preset": "1.1.1",
    "typescript": "4.5.4",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "dependencies": {
    "@wonderflow/icons": "*",
    "ahooks": "3.1.5",
    "clsx": "1.1.1",
    "framer-motion": "^5.5.7",
    "react-data-table-component": "7.4.6",
    "react-focus-on": "3.5.4",
    "react-loading-skeleton": "3.0.2",
    "react-masonry-css": "1.0.16",
    "react-paginate": "8.1.0",
    "react-popper-tooltip": "4.3.1",
    "react-roving-tabindex": "3.1.1",
    "react-uid": "2.3.1",
    "styled-components": "5.3.3",
    "type-fest": "2.9.0"
  }
}
